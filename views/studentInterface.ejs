                        <label for="title">Tiêu đề bài đăng</label>
                        <input value="" class="form-control" type="text" placeholder="Nhập tên người dùng" id="title" name="title">
                    </div>
                    <div class="form-group">
                        <label for="context">Nhập nội dung</label>
                        <textarea name="txtContent" id="txtContent">Type some text here.</textarea>
                    </div>
                    <div style="display: none;" id="error" class="alert alert-danger">
